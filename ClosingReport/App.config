<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="accounts" type="ClosingReport.AccountsConfiguration, ClosingReport" />
  </configSections>
  <system.diagnostics>
    <sources>
      <source name="ClosingReport" switchName="sourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="console" type="System.Diagnostics.ConsoleTraceListener">
            <filter type="System.Diagnostics.EventTypeFilter" initializeData="Verbose"/>
          </add>
          <add name="myListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="C:\myListener.log">
            <filter type="System.Diagnostics.EventTypeFilter" initializeData="Verbose"/>
          </add>
          <remove name="Default"/>
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="sourceSwitch" value="Warning"/>
    </switches>
    <sharedListeners>
    </sharedListeners>
  </system.diagnostics>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <accounts>
    <account name="Sam's Club" codes="2010" />
    <account name="Fred Meyers" codes="2006" />
    <account name="Costco Photo" codes="2002" />
    <account name="Costco HACS" codes="2090" />
    <account name="Costco RX" codes="2013,2092" />
    <account name="Costco Order Support" codes="2008" />
    <account name="DNP" codes="2007" />
    <account name="Duane Reade" codes="2005" />
    <account name="Mike's Camera" codes="2009" />
    <account name="Other" codes="" />
  </accounts>
</configuration>